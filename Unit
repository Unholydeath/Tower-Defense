package towerDefense.Model;

public abstract class Unit
{
		private int hitPoints = 1;
	
	private int lifePoints = 5;

	private int speed = 2;
	private int x;
	private int y;

	public int GetHitPoints()
	{
		return hitPoints;
	}

	public void StartRunning()
	{
		x += speed;
		y += speed;
	}

	public void AttackEnd()
	{
		//if(x == end && y == end){
		//	life - hitPoints;
		//}
		
		if(lifePoints == 0){
			System.out.println("Dead");
		}
	}

}
